  "devDependencies": {
    "@studiometa/eslint-config": "4.0.1",
    "@studiometa/prettier-config": "4.1.0",
    "bun": "1.1.38",
    "eslint": "9.4.0",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "chalk": "^5.3.0",
